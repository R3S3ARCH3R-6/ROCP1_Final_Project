<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Bank_Assignment</display-name>
  <welcome-file-list>
    <welcome-file>home_login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>LoginController</display-name>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>com.app.controllers.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/loginUser</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SuccessServletController</display-name>
    <servlet-name>SuccessServletController</servlet-name>
    <servlet-class>com.app.controllers.SuccessServletController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SuccessServletController</servlet-name>
    <url-pattern>/success</url-pattern>
  </servlet-mapping>
</web-app>